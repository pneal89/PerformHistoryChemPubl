<table class="table table-striped">
  <thead>
  <tr>
    <th>id</th>
    <th>email</th>
    <th>date created</th>
    <th>approved</th>
    <th>options</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= (user.approved)? 'yes':'no' %></td>
        <td>

        </td>
      </tr>
  <% end %>
  </tbody>
</table>